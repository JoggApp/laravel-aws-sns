{
    "name": "joggapp/laravel-aws-sns",
    "description": "Laravel package for the SNS events by AWS",
    "keywords": [
        "laravel",
        "package",
        "aws",
        "sns",
        "aws sns",
        "aws sns events"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Harish Toshniwal",
            "email": "harish@jogg.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoggApp\\AwsSns\\": "src"
        }
    },
    "require": {
        "php": "^8.3",
        "aws/aws-php-sns-message-validator": "^1.6",
        "illuminate/http": "^12.0",
        "illuminate/queue": "^12.0",
        "illuminate/support": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoggApp\\AwsSns\\AwsSnsServiceProvider"
            ],
            "aliases":{
                "AwsSns": "JoggApp\\AwsSns\\AwsSnsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
